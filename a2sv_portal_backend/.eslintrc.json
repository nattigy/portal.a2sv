{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "no-useless-constructor": "off",
        "no-unused-vars": "off",
        "eslint-disable-next-line": "off"
    }
}
